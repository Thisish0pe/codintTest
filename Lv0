#<문제 43>
#https://school.programmers.co.kr/learn/courses/30/lessons/181919
#<풀이 1>
def solution(n):
    array = [n]
    while n != 1:
        if n % 2 == 0:
            n = n // 2
        else:
            n = n*3+1
        array.append(n)
    return array
#<풀이 4>
def solution(n):
    array = [n]
    while n != 1:
        n = n//2 if n % 2 ==0 else n*3+1
        array.append(n)
    return array

#2023.06.06
#<문제 44>
#https://school.programmers.co.kr/learn/courses/30/lessons/181918
#<풀이 1>
def solution (arr):
    stk = []
    i = 0

    while i < len(arr):
        if len(stk) == 0:
            stk.append(arr[i])
            i += 1
        elif len(stk) != 0 and stk[-1] < arr[i]:
            stk.append(arr[i])
            i += 1
        elif len(stk) != 0 and stk[-1] >= arr[i]:
            stk.pop()
    return stk
#<풀이 2>
def solution(arr):
    stk = []
    for i in range(len(arr)):
        while stk and stk[-1] >= arr[i]:
            stk.pop()
        stk.append(arr[i])
    return stk

