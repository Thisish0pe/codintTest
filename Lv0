#<문제 11>
#https://school.programmers.co.kr/learn/courses/30/lessons/120849
#<풀이 1>
def solution(my_string):
    return my_string.replace('a','').replace('e','').replace('i','').replace('o','').replace('u','')
print(solution('bus'))
#<풀이 2>
def solution(my_string):
    string='aeiou'
    for i in string:
        my_string = my_string.replace(i,'')
    return my_string
print(solution('mayo'))
#<풀이 3>
import re

def solution(my_string):
    return re.sub(r"[aeiou]","",my_string)
    
print(solution('hahaha'))

#2023 .05. 16
#<문제 12>
#https://school.programmers.co.kr/learn/courses/30/lessons/120851
#<풀이 1>
import re

def solution(data):
    p = re.compile(r'([^a-zA-Z])')
    matches = p.findall(data)
    result = sum([int(match) for match in matches])
    return result

data="aAb1B2cC34oOp"
solution(data)
#<풀이 2>
def solution (data):
    return sum([int(i) for i in data if i.isdigit()])

data="aAb1B2cC34oOp"
solution(data)
#<풀이 3>
def soultion (data):
    return sum(map(int,filter(lambda x:x.isdigit(), data)))

data="aAb1B2cC34oOp"
solution(data)
#<풀이 4>
import re

def solution(my_string):
    return sum([int(i) for i in re.sub('[^1-9]', '', my_string)])
    
 #<문제 13>
 #https://school.programmers.co.kr/learn/courses/30/lessons/120891
 #<풀이 1>
 import re

def solution (order):
    p = re.compile('[369]')
    m = p.findall(order)
    result = len(m)
    return result

order='29423'
print(solution(order))
#<풀이 2>
def solution(order):
    s = []
    for i in order:
        if int(i) == 3 or int(i) == 6 or int(i) == 9:
            s.append(i)
    result = len(s)
    return result

order = '29423'
print(solution(order))
#<풀이 3>
def solution(order):
    s = 0
    for i in str(order):
        if int(i) == 3 or int(i) == 6 or int(i) == 9:
            s += 1
    return s

order = '29423'
print(solution(order))
#<풀이 4>
import re

def solution(order):
    p = re.compile('[369]')
    return len(p.findall(str(order)))

order = '29423'
print(solution(order))

