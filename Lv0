#<풀이 1>
def solution (a, b, flag):
    if flag == True:
        result = a+b
        return result
    else:
        result = a-b
        return result

print(solution(-4,7,True))
#<풀이 2>
def solution(a, b, flag):
    return a + b if flag else a - b

