#<문제 34>
#https://school.programmers.co.kr/learn/courses/30/lessons/181928
#<풀이 1>
def solution (num_list):
    li1=[]
    li2=[]
    for i in num_list:
        if i%2 == 0:
            li1.append (str(i))
        elif i%2 == 1:
            li2.append (str(i))
    result = int(''.join(li1)) + int(''.join(li2))
    return result
#<풀이 2>
def solution (num_list):
    a = ''
    b = ''
    if i % 2 == 0:
        a += str(i)
    else:
        b += str(i)
    return int(a) + int(b)
    
