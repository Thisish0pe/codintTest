# <풀이2>
str = input()

while True:
    if len(str) >= 1 and len(str) <= 100000 and str != ' ':
        print(str)
        break
    else:
        continue

#<문제>
#https://school.programmers.co.kr/learn/courses/30/lessons/181951?language=python3
#<풀이1>
a,b = map(int, input().strip().split())
print(f'a={a}\nb={b}')
#<풀이2>
input_str = input().strip()
input_list = input_str.split(' ')
a = int(input_list[0])
b = int(input_list[1])
print(f"a = {a}\nb = {b}")
