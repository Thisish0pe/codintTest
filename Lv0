def solution (a, d, included):
    total = 0
    for i in range(len(included)):
        if included[i]:
            term += a + (i *d)
            total += term
    return total
#<풀이 2>
def solution (a, d, included):
    answer = 0
    for i in range(len(included)):
        answer += (a + i *d) * included[i]
    return answer

