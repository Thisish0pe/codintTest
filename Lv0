#<풀이 1>
def solution(l, r):
    result =[]
    for i in range(l, r+1):
        if all(digit in {'5', '0'} for digit in str(num)):
            result.append(num)
        return result if result else [-1]
#<풀이 2>
def solution(l, r):
    answer = []
    for num in range(l, r + 1):
        if not set(str(num)) - set(['0', '5']):
            answer.append(num)
    return answer if answer else [-1]
    
