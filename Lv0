import math
def solution (num_list):
    if sum(num_list)**2 > math.prod(num_list):
        return 1
    else: 
        return 0
#<풀이 2>
def solution (num_list):
    s = sum(num_list)**2
    m = eval('*'.join([str(i) for i in num_list]))
    return 1 if s > m else 0
    
