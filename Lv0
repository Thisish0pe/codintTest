#<풀이 1>
def solution(str1, str2):
    answer = ''
    for i in range(0,len(str1)):
        answer = answer + str1[i] + str2[i]
    return answer
#<풀이 2>
def solution (str1, str2):
    result = ''
    for i,j in zip(str1, str2):
        result += i + j
    return result

str1 = 'aaaaa'
str2 = 'bbbbb'
print(solution(str1, str2))

